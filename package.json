{
  "name": "dcg-edu-backend",
  "version": "1.0.0",
  "description": "Backend da API da DCG-EDU",
  "main": "server.js",
  "scripts": {
    "gen:all": "node scripts/generate-migrations-seeders.js",
    "fix-models": "node scripts/fixModels.js",
    "gen:migrations": "node scripts/generate-migrations-from-models.js",
    "dev": "nodemon server.js",
    "start": "node server.js",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "test": "jest --runInBand",
    "test:unit": "jest tests/unit --runInBand",
    "test:e2e": "jest tests/e2e --runInBand",
    "server": "nodemon server.js --ext js,json",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:sync": "node scripts/syncDatabase.js"
  },
  "type": "commonjs",
  "engines": {
    "node": "20.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dcgedusmilles-code/backend-dcg-edu.git"
  },
  "author": "Jo√£o Jamba - kaizen-TI",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jimp": "^0.22.10",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^6.7.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.4.1",
    "nodemailer": "^6.8.0",
    "sequelize": "^6.37.7",
    "slugify": "^1.6.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/register": "^7.28.3",
    "@types/swagger-ui-express": "^4.1.6",
    "eslint": "^9.28.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4"
  }
}
